{
  "name": "custom-react-datepicker",
  "version": "1.0.0",
  "description": "Customizable ranged (or not!) datepicker built in react.",
  "author": "Kristianna Elbert",
  "license": "ISC",
  "private": true,
  "peerDependencies": {
    "react": "^16.12.0",
    "react-dom": "^16.12.0"
  },
  "dependencies": {
    "@types/node": "^12.12.28",
    "@types/react": "^16.9.27",
    "@types/react-dom": "^16.9.5",
    "awesome-typescript-loader": "^5.2.1",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-scripts": "3.4.0",
    "source-map-loader": "^0.2.4",
    "typescript": "^3.8.3"
  },
  "main": "build/index.js",
  "module": "build/index.es.js",
  "jsnext:main": "build/index.es.js",
  "files": [
    "build"
  ],
  "scripts": {
    "start": "react-scripts start",
    "build": "rm -rf ./build && rollup -c",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "tslint -c tslint.json --project tsconfig.json",
    "prepare": "npm test && npm lint && npm build",
    "eject": "react-scripts eject"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.5.0",
    "@testing-library/user-event": "^7.2.1",
    "@types/enzyme": "^3.10.5",
    "@types/enzyme-adapter-react-16": "^1.0.6",
    "@types/jest": "^25.0.0",
    "@types/react-axe": "^3.1.0",
    "@types/react-test-renderer": "^16.9.2",
    "@types/testing-library__jest-dom": "^5.0.2",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "eslint-plugin-jest-dom": "^2.0.1",
    "jest-dom": "^4.0.0",
    "react-axe": "^3.4.1",
    "react-test-renderer": "^16.13.1",
    "rollup": "^2.0.2",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-peer-deps-external": "^2.2.2",
    "rollup-plugin-typescript2": "^0.26.0",
    "ts-jest": "^25.3.0",
    "tslint": "^6.0.0",
    "tslint-react": "^4.2.0",
    "webpack-cli": "^3.3.11"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "plugin:jsx-a11y/recommended"
    ],
    "plugins": [
      "jsx-a11y"
    ]
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node"
  }
